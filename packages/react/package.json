{
  "name": "@ace-ex/react",
  "version": "1.0.1",
  "license": "MIT",
  "description": "Biblioteca de componentes React.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "directory": "packages/react",
    "url": "git+https://github.com/ShadowsS01/ace-ex-ui.git"
  },
  "bugs": {
    "url": "https://github.com/ShadowsS01/ace-ex-ui/issues"
  },
  "homepage": "https://github.com/ShadowsS01/ace-ex-ui/tree/main/packages/react/README.md",
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --dts --external react --watch",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf .turbo"
  },
  "dependencies": {
    "@ace-ex/tokens": "*",
    "@radix-ui/react-alert-dialog": "^1.0.2",
    "@radix-ui/react-avatar": "^1.0.1",
    "@radix-ui/react-checkbox": "^1.0.1",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-dropdown-menu": "^2.0.1",
    "@radix-ui/react-select": "^1.1.2",
    "@radix-ui/react-tabs": "^1.0.1",
    "@stitches/react": "^1.2.8",
    "phosphor-react": "^1.4.1",
    "polished": "^4.2.2",
    "react-spring": "^9.6.1"
  },
  "devDependencies": {
    "@ace-ex/eslint-config": "*",
    "@ace-ex/ts-config": "*",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "4.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
